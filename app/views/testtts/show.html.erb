<%- model_class = Testtt -%>
<% title t '.title', :default => model_class.model_name.human -%>

<div class="form-group form-group-admin-buttons">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              admin_testtts_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_testtt_path(@testtt), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_ @testtt,
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm")) },
              :class => 'btn btn-danger' %>
  <%= link_to t('helpers.links.admin'), admin_path, :class => 'btn btn-default' %>
</div>

<p>
  <strong><%= model_class.human_attribute_name(:name) %>:</strong>
  <%= @testtt.name %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:description) %>:</strong>
  <%= @testtt.description %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:age) %>:</strong>
  <%= number_with_delimiter(number_with_precision(@testtt.age)) %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:worth) %>:</strong>
  <%= number_with_delimiter(number_with_precision(@testtt.worth)) %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:is_male) %>:</strong>
  <%= @testtt.is_male %>
</p>
<p>
  <strong><%= model_class.human_attribute_name(:born_at) %>:</strong>
  <%= l @testtt.born_at if .born_at.present? %>
</p>
